#!/usr/bin/env php
<?php
declare( strict_types = 1 );

require_once __DIR__ . '/../bootstrap.php';

use Symfony\Component\Console\Application;
use Cyclopol\Command\DownloadArticles;
use Cyclopol\Command\NameStreets;
use Cyclopol\Command\ArticlesFromSources;

$application = new Application();

$application->add( new DownloadArticles( $entityManager ) );
$application->add( new ArticlesFromSources( $entityManager ) );
$application->add( new NameStreets( $entityManager ) );

$application->run();
