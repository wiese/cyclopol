#!/usr/bin/env php
<?php
declare( strict_types = 1 );

require_once __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Cyclopol\Command\DownloadArticles;
use Cyclopol\Command\NameStreets;

$application = new Application();

$application->add( new DownloadArticles() );
$application->add( new NameStreets() );

$application->run();
